public double[] DoubleUnion() { double[] numbersA = { 0.1, 2.2, 4.3, 5.4, 6.5, 8.6, 9.7 }; double[] numbersB = { 0.1, 3.3, 5.4, 7.7, 8.6 }; double[] uniqueNumbers = numbersA.Union(numbersB).ToArray(); return uniqueNumbers; }
