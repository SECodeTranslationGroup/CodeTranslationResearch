public int[] ProjectionIntUnion() { int[] numbersA = { 0, 2, 4, 5, 6, 8, 9 }; int[] numbersB = { 1, 3, 5, 7, 8 }; var first = numbersA.Select(n => n+1); var second = numbersB.Select(n => n-1); int[] numbers = first.Union(second).ToArray(); return numbers; }
